<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ssh.web.student.service">
    <class name="Student" table="stu_table">
        <id name="studentId" column="stu_id">
            <generator class="identity"/>
        </id>
        <property name="name" column="stu_name" type="string"/>
        <property name="age" column="stu_age" type="int"/>
    </class>

</hibernate-mapping>
